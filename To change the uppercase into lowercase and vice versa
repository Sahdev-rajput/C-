#include <iostream>
#include <string>
#include <cstring>
using namespace std;
int main()
{
	string str = "WeLcoMe5";
	for (int i = 0; i < str.length(); i++)
	{
		if (str[i] < 65)
		{
			str[i] = str[i];
		}
		else if (str[i] >= 97)
		{
			str[i] = str[i] - 32;
		}
		else
		{
			str[i] = str[i] + 32;
		}
	}
	cout << str << endl;
	return 0;
}
	
