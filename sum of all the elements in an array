#include <iostream>
using namespace std;
int main()
{
	int n;
	int A[5];
	n = sizeof(A)/sizeof(A[1]);
	cout << n << endl;
	cout << "enter elelmnts of array";
	for (int i = 0; i < 5; i++)
	{
		cin >> A[i];
	}
	int sum = 0;
	for (int i = 0; i < 5;i++)
	{
		sum = sum + A[i];
	}
	cout << sum;
	return 0;
}
